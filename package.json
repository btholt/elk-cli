    "columnify": "1.5.4",
    "configstore": "2.0.0",
    "fs-extra": "0.30.0",
    "hasbin": "1.2.3",
    "meow": "3.7.0",
    "pify": "2.3.0",
    "tmp": "0.0.28"
  },
  "devDependencies": {
    "standard": "7.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btholt/elk-cli.git"
  },
